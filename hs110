#include <stdio.h>
void main() 
{

	int i,j,n,m,temp=0;
	int a1[100],a2[100];
	scanf("%d",&n);
	scanf("%d",&m);
	for(i=0;i<n;i++)
	{
		scanf("%d",&a1[i]);
	}
	for(i=0;i<m;i++)
	{
		
		scanf("%d",&a2[i]);
	}
	
    for(i=0;i<n;i++)
    {
        for(j=0;j<m;j++)
        {
            if(a1[i]==a2[j])
             {
 	            temp++;
             }
        }
    }
    if(temp==n)
        {
            printf("YES");
        }
    else
        {
            printf("NO");
        }		
}
